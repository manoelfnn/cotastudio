{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "CodeMirror": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
     "prettier/prettier": "error",
     "no-continue": "off"
  }
}